{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/components/*"],
            "@constants/*": ["src/constants/*"],
            "@css/*": ["src/css/*"],
            "@utils/*": ["src/utils/*"],
            "@content/*": ["src/content/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    }
}
