{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/components/*"],
            "@constants/*": ["src/constants/*"],
            "@css/*": ["src/css/*"],
            "@utilities/*": ["src/utilities/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    }
}
